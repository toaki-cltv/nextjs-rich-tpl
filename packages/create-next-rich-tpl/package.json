{
  "name": "create-next-rich-tpl",
  "version": "0.0.1",
  "description": "Central create command that lists available templates and delegates to per-template create packages or local templates",
  "type": "module",
  "bin": {
    "create-next-rich-tpl": "bin/create.mjs"
  },
  "files": [
    "bin/"
  ],
  "keywords": [
    "create",
    "template",
    "nextjs"
  ],
  "license": "MIT",
  "author": "toaki-cltv",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toaki-cltv/nextjs-rich-tpl.git"
  },
  "scripts": {
    "test": "node -e \"console.log('no tests')\"",
    "pre": "pnpm version prerelease --preid=beta -m 'chore(release): %s'",
    "pub:beta": "pnpm publish --access public --tag beta",
    "pub:latest": "pnpm publish --access public --tag latest"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.2",
    "degit": "^2.8.4",
    "inquirer": "^12.10.0",
    "ora": "^9.0.0"
  },
  "config": {
    "templatesIndexUrl": "https://raw.githubusercontent.com/toaki-cltv/nextjs-rich-tpl/main/templates/index.json"
  }
}
